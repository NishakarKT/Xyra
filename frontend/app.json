{
  "expo": {
    "name": "Startup",
    "slug": "startup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "dark",
    "scheme": "startup",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#041F30"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
          "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/images/icon.png",
          "backgroundColor": "#041F30"
        },
        "edgeToEdgeEnabled": true
      },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#041F30"
        }
      ]
    ]
  }
}